case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../stylus/bin/stylus" "$@"
  ret=$?
else 
  node  "$basedir/../stylus/bin/stylus" "$@"
  ret=$?
fi
exit $ret
